{#
/**
 * @file location_entity.html.twig
 * Default theme implementation to present Location entity data.
 *
 * This template is used when viewing Location entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_location_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('location_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
